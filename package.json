{
  "name": "electron-boilerplate",
  "version": "0.1.0",
  "description": "A Bolierplate application for Electron runtime",
  "main": "build/main.js",
  "scripts": {
    "postintall": "cd app && npm install && cd ..",
    "webpack": "webpack --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.renderer.js --hot --inline --colors",
    "clean": "rimraf build",
    "preboot": "npm run clean && cross-env NODE_ENV=development npm run webpack -- --config webpack.config.main.js",
    "boot": "cross-env NODE_ENV=development electron build",
    "build:main": "npm run webpack -- --config webpack.config.main.js -p",
    "build:renderer": "npm run webpack -- --config webpack.config.renderer.js -p",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build:main && cross-env NODE_ENV=production npm run build:renderer",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron build",
    "release": "node tasks/release"
  },
  "dependencies": {
    "fs-jetpack": "^0.9.2",
    "vue": "^1.0.26",
    "vue-router": "^0.7.13"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "^1.2.6",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/electron-boilerplate.git"
  },
  "keywords": [
    "iceStone",
    "wedn",
    "micua",
    "electron",
    "vue",
    "webpack"
  ],
  "author": "iceStone <ice@micua.com> (http://blog.wedn.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zce/electron-boilerplate/issues"
  },
  "homepage": "https://github.com/zce/electron-boilerplate#readme"
}
